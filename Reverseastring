public class Reverseastring
{
public static void main(String[] args)
{
String s="Java is a typed programming language";
String[] split= s.split(" ");//Split the string
String result="";
for(int i=split.length-1;i>=0;i--)//for loop to reverse the string
{
result +=(split[i] +" ");
}
System.out.println(result.trim());//print the reverse string
}//end of main
}//end of class
